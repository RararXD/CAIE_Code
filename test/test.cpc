DECLARE variable : INTEGER
variable <- 10
OUTPUT "Variable", variable

DECLARE array : ARRAY[1:5] OF INTEGER
array[1] <- 1
array[2] <- 2
array[3] <- 3
array[4] <- 4
array[5] <- 5
array <- [6, 7, 8, 9, 10]
OUTPUT "Array", array

DECLARE counter : INTEGER
WHILE counter < 5 DO
  counter <- counter + 1
ENDWHILE
OUTPUT "Counter", counter

DECLARE i : INTEGER
FOR i <- 1 TO 5
  OUTPUT "i is", i
NEXT i

DECLARE number : INTEGER
number <- 7
IF number < 10 THEN
  OUTPUT "Number is less than 10"
ENDIF

